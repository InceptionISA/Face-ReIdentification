FACENET_MODEL_PATH="models/facenet/20180402-114759.pb"


# =============== MODELS Paths ==================

FACE_DETECTION_BACKEND="mtcnn"
DLIB_DETECTOR_MODEL_PATH="models/dlib/mmod_human_face_detector.dat"

# ===============  Config ==================

MTCNN_MIN_FACE_SIZE=20



